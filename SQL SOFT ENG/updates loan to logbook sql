Use `Soft Eng 2`;

-- 1. Turn off safe updates temporarily
SET SQL_SAFE_UPDATES = 0;

-- 2. Run the update to fix the NULL values
UPDATE LogBook 
SET Availability = 'Available' 
WHERE Availability IS NULL OR Availability = '';

-- 3. Turn safe updates back on (good practice)
SET SQL_SAFE_UPDATES = 1;
